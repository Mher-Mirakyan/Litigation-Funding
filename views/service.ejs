<%- include('partials/header'); %>

    <!-- Section 1 -->
    <div style="background-image: url('<%= sec_1_img %>')" class="container-fluid bg-cover bg-no-repeat bg-center py-40 relative">
        <div class="absolute h-full w-full top-0 left-0" style="background:rgba(0,0,0,0.5)"></div>
            <h2 class="text-5xl font-bold text-white text-center relative z-30"><%= headline %></h2>
       </div>
    
       <!-- Section 2 -->
       <div class="container-fluid bg-white pt-10 pb-48">
        <div class="mx-auto text-center flex justify-center">
            <p class="text-mainColor text-3xl lg:w-2/3"><%= sec_2_title %></p>
        </div>
        <div class="container mx-auto py-10 lg:flex lg:justify-around">
            <div class="w-full lg:w-1/2 py-10 px-10">
                <p><%- sec_2_block_1_paragraph_1 %><br><br>
    
                  <%= sec_2_block_1_paragraph_2 %> <br><br>
                  <%= sec_2_block_1_paragraph_3 %> </p>
            </div>
            <div class="w-full lg:w-1/2 py-10 px-10">
                <p><%= sec_2_block_2_paragraph_1 %><br><br>
    
                    <%= sec_2_block_2_paragraph_2 %><br><br>
                    <%= sec_2_block_2_paragraph_3 %>
                 </p>
            </div>
        </div>
       </div>
    
        <!-- Section 3 -->
        <div class="container-fluid relative bg-gray-100 pt-28 pb-12">
            <div class="w-full mx-auto flex justify-center items-center absolute  -top-[6%] sm:-top-[7%] md:-top-[12%] lg:-top-[30%]">
                <img src="/money-3-1024x334.png.webp" alt="" class="">
           </div>
           <div class="container mx-auto lg:flex lg:justify-around">
            <div class="w-full lg:w-1/2 py-10 px-12">
                <h4 class="text-mainColor font-bold text-3xl"><%= sec_3_title %></h4>
                <p class="text-lg py-5"><%= sec_3_paragraph_1 %><br><br>
    
                   <%= sec_3_paragraph_2 %> <br><br>
                    
                     <%- sec_3_paragraph_3 %> </p>
            </div>
            <div class="w-full lg:w-1/2 py-10 px-12 flex justify-between">
                <ul class="list-disc text-lg text-mainColor leading-10 w-1/2">
                    <% if (locals.listItems && listItems.length > 0) { %>
                        <% listItems.forEach(function(item, index) { %>
                            <li> <%= item %> </li>


                        <% }); %>
                    <% } %>
                </ul>
                <ul class="list-disc text-lg text-mainColor leading-10 w-1/2">
                    <% if (locals.listItems2 && listItems2.length > 0) { %>
                        <% listItems2.forEach(function(item, index) { %>
                            <li> <%= item %> </li>


                        <% }); %>
                    <% } %>
                </ul>
            </div>
           </div>
       </div>
    
        <!-- Section 4 -->
        <div class="container-fluid bg-mainColor">
            <div class="container mx-auto py-20 text-center">
                <h3 class="text-white text-5xl font-bold mb-10"><%= sec_4_title %></h3>
                <p class="text-2xl italic px-10 text-white"><%- sec_4_subtitle %></p>
            </div>
        </div>
    
    <!-- Section 5 -->
    <div class="container-fluid bg-white py-12">
        <div class="container mx-auto px-12 lg:px-44">
            <h6 class="text-lg font-bold text-mainColor py-5 px-5 bg-slate-100"><%= sec_5_question_1 %></h6>
            <p class="text-lg"><%= sec_5_answer_1 %></p>
            <hr>
            <br>
            <h6 class="text-lg font-bold text-mainColor py-5 px-5 bg-slate-100"><%= sec_5_question_2 %></h6>
            <br>
            <p class="text-lg"><%= sec_5_answer_2_1 %> <br><br>
    
              <%= sec_5_answer_2_2 %>  </p>
            <hr><br>
            <h6 class="text-lg font-bold text-mainColor py-5 px-5 bg-slate-100"><%= sec_5_question_3 %></h6>
            <p class="text-lg"><%= sec_5_answer_3_1 %> <br><br>
    
              <%= sec_5_answer_3_2 %>  </p>
            <hr><br>
            <span class="text-sm"><%= sec_5_span %></span>
    
        </div>
    </div>
    
        <!-- Section 6 -->
       <div style="background-image: url('<%= sec_6_img %>')" class="container-fluid py-32 bg-cover bg-no-repeat bg-center relative">
        <div class="absolute h-full w-full top-0 left-0" style="background:rgba(0,0,0,0.5)"></div>
        <div class="container mx-auto relative z-40 text-center">
            <h2 class="text-white text-6xl font-bold"><%= sec_6_title %></h2>
            <div class="w-full container mx-auto lg:flex lg:justify-center px-4 mt-3">
                <button class="bg-mainColor text-white text-xl font-bold rounded-full w-full lg:w-2/12 lg:mr-2 mb-2 py-3 lg:px-16 duration-300 hover:bg-white hover:text-black"><%= sec_6_btn1 %></button>
                <button class="bg-mainColor text-white text-xl font-bold rounded-full w-full lg:w-2/12 lg:ml-2 mb-2 py-3 lg:px-16 duration-300 hover:bg-white hover:text-black"><%= sec_6_btn2 %></button>
            </div>
        </div>
       </div>
    
       <%- include('partials/registrationForm', {info: registrationForm}) ; %>


<%- include('partials/footer'); %>